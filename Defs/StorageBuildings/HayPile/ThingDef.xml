<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="ASNeolithicStorageBase">
		<defName>ASNeolithicHayPile</defName>
		<label>hay pile</label>
		<graphicData>
			<texPath>ASNeolithic/HayPile/HayPileStage1</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3,3)</drawSize>
			<drawRotated>false</drawRotated>
		</graphicData>
		<description>A pile of hay for easier storage.</description>
		<fillPercent>0.10</fillPercent>
		<uiIconPath>ASNeolithic/HayPile/HayPile_UI_Icon</uiIconPath>
		<uiIconScale>1</uiIconScale>
		<size>(2,1)</size>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>30</MaxHitPoints>
			<WorkToBuild>300</WorkToBuild>
			<Flammability>2</Flammability>
			<Beauty>-2</Beauty>
			<Mass>3</Mass>
		</statBases>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>10</costStuffCount>
		<costList>
			<Hay>75</Hay>
		</costList>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<building>
			<maxItemsInCell>3</maxItemsInCell>
			<fixedStorageSettings>
				<filter>
					<disallowNotEverStorable>true</disallowNotEverStorable>
					<thingDefs>
						<li>Hay</li>
					</thingDefs>
				</filter>
			</fixedStorageSettings>
		</building>
	</ThingDef>
</Defs>