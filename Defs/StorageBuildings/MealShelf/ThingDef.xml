<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="ASNeolithicStorageBase">
		<defName>ASNeolithicMealShelf</defName>
		<label>meal shelf</label>
		<graphicData>
			<texPath>ASNeolithic/MealShelf/MealShelf</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
			<drawRotated>false</drawRotated>
		</graphicData>
		<description>A makeshift shelf for the purposes of meal storage.</description>
		<fillPercent>0.20</fillPercent>
		<uiIconPath>ASNeolithic/MealShelf/MealShelf_UI_Icon</uiIconPath>
		<uiIconScale>1.1</uiIconScale>
		<size>(1,1)</size>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>95</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
			<Flammability>1</Flammability>
			<Beauty>0.5</Beauty>
			<Mass>4</Mass>
		</statBases>
		<costStuffCount>10</costStuffCount>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<staticSunShadowHeight>0.5</staticSunShadowHeight>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<building>
			<maxItemsInCell>3</maxItemsInCell>
			<fixedStorageSettings>
				<filter>
					<categories>
						<li>FoodMeals</li>
					</categories>
				</filter>
			</fixedStorageSettings>
			<paintable>true</paintable>
		</building>
	</ThingDef>
</Defs>